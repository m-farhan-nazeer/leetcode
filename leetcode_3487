class Solution(object):
    def maxSum(self, nums):
        #made a set ,made maxi to store maximum number to display in case of negative numbers 
        subarray = set()
        poz = False 
        maxi =  float('-inf')
        for i in range(len(nums)):
            if nums[i]>=0:
                poz = True 
            if nums[i] > maxi :
                maxi = nums[i]
            subarray.add(nums[i])  

        #if all positive handled normallY
        if poz:
            x = 0
            for val in subarray:      
                if val > 0  :
                    x += val
            
            return x
        #if all negative just return negative maximum
        
        return maxi
       
